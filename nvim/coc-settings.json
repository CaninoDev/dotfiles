{
  // coc settting
  "coc.preferences.triggerAfterInsertEnter": false,
  "coc.preferences.diagnostic.enable": true,
  "coc.preferences.snippets.enable": true,
  "coc.source.ultisnips.enable": true,
  "coc.source.neosnippet.enable": false,
  // prettier
  "prettier.eslintIntegration": true,
  "prettier.tslintIntegration": true,
  "prettier.stylelintIntegration": true,
  // eslint
  "eslint.filetypes": [
    "javascript",
    "javascript.jsx"
  ],
  "eslint.autoFix": true,
  "eslint.autoFixOnSave": true,
  "tslint.autoFixOnSave": true,
  // tsserver
  "tsserver.implicitProjectConfig.experimentalDecorators": true,
  // languageserver
  "languageserver": {
    "sh": {
      "command": "bash-language-server",
      "args": ["start", "--stdio"],
      "filetypes": ["sh"],
      "cwd": "./",
      "initializationOptions": {},
      "settings": {}
    },
    "dart": {
      "command": "dart_language_server",
      "args": [],
      "filetypes": ["dart"],
      "cwd": "./lib",
      "initializationOptions": {},
      "settings": {}
    }
  }
  // vim:ft=jsonc
}
