{
  "Coc-Package-Example": {
    "prefix": "cocpe",
    "body": [
      "{",
      "  \"name\": \"coc-${1:name}\",",
      "  \"version\": \"0.0.1\",",
      "  \"description\": \"${2:description}\",",
      "  \"keywords\": [",
      "    \"coc.nvim\",",
      "    \"${3:keyword}\"",
      "  ],",
      "  \"repository\": \"git@github.com:iamcco/coc-${1:name}.git\",",
      "  \"author\": \"iamcco <ooiss@qq.com>\",",
      "  \"license\": \"MIT\",",
      "  \"engines\": {",
      "    \"coc\": \"${4:0.0.67}\"",
      "  },",
      "  \"activationEvents\": [",
      "    \"*\"",
      "  ],",
      "  \"contributes\": {",
      "    \"configuration\": {",
      "      \"type\": \"object\",",
      "      \"properties\": {",
      "        \"${1:name}.enable\": {",
      "          \"type\": \"boolean\",",
      "          \"default\": true,",
      "          \"description\": \"Is enable coc-${1:name}\"",
      "        },",
      "        \"${1:name}.trace.server\": {",
      "          \"type\": \"string\",",
      "          \"default\": \"off\",",
      "          \"enum\": [",
      "            \"off\",",
      "            \"messages\",",
      "            \"verbose\"",
      "          ],",
      "          \"description\": \"Trace level of coc-${1:name} server\"",
      "        }",
      "      }",
      "    }",
      "  },",
      "  \"main\": \"./out/index.js\",",
      "  \"scripts\": {",
      "    \"build\": \"tsc -p .\"",
      "  },",
      "  \"devDependencies\": {",
      "    \"@types/node\": \"^12.0.4\",",
      "    \"coc.nvim\": \"^0.0.68\",",
      "    \"typescript\": \"^3.5.1\"",
      "  },",
      "  \"dependencies\": {",
      "    \"vscode-languageserver-protocol\": \"^3.14.1\"",
      "  }",
      "}"
    ],
    "description": "coc package example"
  }
}
